{
    "compilerOptions": {
      "outDir": "./dist/",
      "baseUrl": ".",
    "paths": {
      "*": ["*", "components/*"]
    },
      "noImplicitAny": true,
      "strictNullChecks": true,
      "strictPropertyInitialization": true,
      "strictFunctionTypes": true,
      "strict": true,
      "sourceMap": false,
      "module": "es6",
      "target": "es5",
      "jsx": "react",
      "allowJs": true,
      "moduleResolution": "node"
    }
  }
